<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
    <lines at="middlleft">
        <line>
            <str>{pingicon}{gold}{username} {white}FPS: </str>
            <op>
                <str>gt</str>
                <var>fps</var>
                <num>60</num>
                <num>45</num>
                <num>30</num>
                <num>15</num>
                <num>0</num>
                <str>{green}</str>
                <str>{yellow}</str>
                <str>{gold}</str>
                <str>{red}</str>
                <str>{darkred}</str>
                <str/>
            </op>
            <var>fps</var>
        </line>
        <line>
            <icon>
                <str>TwilightForest:item.oreMeter</str>
            </icon>
            <str> Minecraft Year {darkaqua}</str>
            <add>
                <round>
                    <div>
                        <sub>
                            <var>day</var>
                            <modi>
                                <var>day</var>
                                <num>365</num>
                            </modi>
                        </sub>
                        <num>365</num>
                    </div>
                    <num>0</num>
                </round>
                <num>1</num>
            </add>
            <str> {white}, Day {darkaqua}</str>
            <modi>
                <var>day</var>
                <num>365</num>
            </modi>
        </line>
        <line>
            <icon>
                    <str>minecraft:clock</str>
            </icon>
        <str>{white} Time: {gold}{mctime12} </str>
        <if>
            <var>daytime</var>
            <str>{yellow}(day)</str>
            <str>{gray}(night)</str>
        </if>
        </line>
        <line>
            <icon>
                <str>HardcoreEnderExpansion:temple_end_portal</str>
            </icon>
            <str> Dimension: {gold}{dimension} ({dimensionid})</str>
        </line>
        <line>
            <icon>
                <str>ChickenChunks:chickenChunkLoader</str>
            </icon>
            <str> Chunks Loaded: {gold}{loadedchunks}</str>
        </line>
        <line>
            <icon>
                <str>minecraft:sapling</str>
                <num>4</num>
            </icon>
            <str> Biome: {darkaqua}{biome} {gold}</str>
            <round>
                <var>localtemperature</var>
                <num>2</num>
            </round>
            <str>{white}°F</str>
        </line>
        <line>
            <str>Temp: {gold}</str>
            <round>
                <div>
                    <sub>
                        <var>localtemperature</var>
                        <num>32</num>
                    </sub>
                    <num>1.8</num>
                </div>
                <num>0</num>
            </round>
            <str>{white}°C {gold}</str>
            <round>
                <add>
                    <div>
                        <sub>
                            <var>localtemperature</var>
                            <num>32</num>
                        </sub>
                        <num>1.8</num>
                    </div>
                    <num>273.15</num>
                </add>
                <num>0</num>
            </round>
            <str>{white}K</str>
            <str> {gold}{humidity}{white}% Humidity</str>
        </line>
        <line>
            <icon>
                <str>minecraft:skull</str>
